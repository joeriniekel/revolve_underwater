#!/bin/bash
while true
	do
         for i in {1..10}; do
                ./revolve.py --experiment-name swimming_up_vert --run $i --manager experiments/swimming_up/manager_vertDisplacement.py --world worlds/empty_underwater.world --z-start -95  --n-cores 1;
                sleep 5s
	              ./revolve.py --experiment-name swimming_up_3d--run $i --manager experiments/swimming_up/manager_3dDisplacement.py --world worlds/empty_underwater.world --z-start -95 --n-cores 1;
                sleep 5s
	   done
done
